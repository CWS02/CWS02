<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - cpc直得科技股份有限公司 CHIEFTEK PRECISION CO., LTD</title>
    <script src="~/Scripts/jquery/jquery-3.7.1.js"></script>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="~/Scripts/vue/vue.min.js"></script>

    <script src="~/Scripts/vue3/vue.min.js"></script>
    <script src="~/Scripts/jquery/jquery-3.7.1.js"></script>
    <link rel="stylesheet" href="~/css/style2CPC.css" />
    <!-- DataTables 核心樣式與腳本 -->
    <script src="~/Scripts/datatable/jquery.dataTables.min.js"></script>
    <link rel="stylesheet" href="~/css/datatable/dataTables.bootstrap5.min.css" />
    <link rel="stylesheet" href="~/css/datatable/jquery.dataTables.min.css" />
</head>

<body>
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid body-content">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("業務", "InterviewRecordList", "Business", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="@Url.Action("InterviewRecordList","Business")">客戶訪談記錄</a></li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>
    @RenderSection("scripts", required: false)
</body>

</html>
